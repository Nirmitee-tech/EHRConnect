    "enableFetalMonitoring": true,
    "enableGeneticScreening": true,
    "enableTTTSMonitoring": true,
    "enableEPDSScreening": true,
    "enablePostpartumCare": true,
    "enableComplicationsTracking": true,
    "enableBabyLinkage": true,
    "enablePregnancyHistory": true,
    "enableLabsTracking": true,
    "enableKickCounts": true,
    "enableBirthPlan": true,
    "enablePregnancyTimeline": true
  },

  "devices": [
    {
      "type": "fetal_monitor",
      "required": false,
      "integration": "hl7"
    },
    {
      "type": "ultrasound",
      "required": false,
      "integration": "dicom"
    }
  ],

  "episodeConfig": {
    "allowConcurrent": false,
    "defaultState": "active",
    "requiredFields": ["edd", "lmp", "gravida", "para"],
    "stateTransitions": {
      "planned": ["active", "cancelled"],
      "active": ["finished", "cancelled"],
      "finished": [],
      "cancelled": []
    }
  }
